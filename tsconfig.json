{
  "compilerOptions": {
    "target": "es2022",
    "module": "commonjs",
    "types": ["node", "jest"],
    "lib": ["es2022"],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["src/**/*.ts"]
}
